CREATE DATABASE edugame;

CREATE TABLE alternativa (
  id_alternativa INTEGER UNSIGNED NOT NULL AUTO_INCREMENT,
  id_pergunta INTEGER UNSIGNED NOT NULL,
  descricao VARCHAR(255) NOT NULL,
  correta BIT NULL,
  PRIMARY KEY(id_alternativa)
);

CREATE TABLE assunto (
  id_assunto INTEGER UNSIGNED NOT NULL AUTO_INCREMENT,
  id_disciplina INTEGER UNSIGNED NOT NULL,
  descricao VARCHAR(255) NOT NULL,
  PRIMARY KEY(id_assunto)
);


CREATE TABLE pergunta (
  id_pergunta INTEGER UNSIGNED NOT NULL,
  descricao VARCHAR(255) NOT NULL,
  id_tipo_pergunta INTEGER UNSIGNED NOT NULL,
  id_assunto INTEGER UNSIGNED NOT NULL,
  id_usuario INTEGER UNSIGNED NOT NULL,
  dia DATE NOT NULL,
  PRIMARY KEY(id_pergunta)
);

CREATE TABLE tipo_pergunta (
  id_tipo_pergunta INTEGER UNSIGNED NOT NULL AUTO_INCREMENT,
  descricao VARCHAR(255) NOT NULL,
  PRIMARY KEY(id_tipo_pergunta)
);

ALTER TABLE alternativa ADD CONSTRAINT fk_pergunta
    FOREIGN KEY(id_pergunta) REFERENCES pergunta(id_pergunta);

ALTER TABLE pergunta ADD CONSTRAINT fk_assunto
    FOREIGN KEY(id_assunto) REFERENCES assunto(id_assunto);

ALTER TABLE pergunta ADD CONSTRAINT fk_tipo_pergunta
    FOREIGN KEY(id_tipo_pergunta) REFERENCES tipo_pergunta(id_tipo_pergunta);

CREATE TABLE `aluno` (
  `idaluno` int(10) UNSIGNED NOT NULL,
  `matricula` varchar(20) NOT NULL,
  `token` varchar(200) DEFAULT NULL,
  `nomealuno` varchar(100) DEFAULT NULL,
  `email` varchar(100) DEFAULT NULL,
  `urlimagem` varchar(200) DEFAULT NULL,
  `senha` varchar(80) DEFAULT NULL,
  `alunoativo` int(1) UNSIGNED DEFAULT NULL
) ENGINE=MyISAM DEFAULT CHARSET=utf8;


